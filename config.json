{
    "log": {
        "loglevel": "none"
    },
    "inbounds": [
        {
            "tag": "http",
            "port": 10808,
            "listen": "0.0.0.0",
            "protocol": "http",
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "routeOnly": false
            },
            "settings": {
                "auth": "noauth",
                "udp": true,
                "allowTransparent": false
            }
        }
    ],
    "outbounds": [
        {{"port":80}}
    ],
    "policy": {
        "levels": {
            "0": {
                "handshake": 2,
                "connIdle": 120
            }
        }
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "domainMatcher": "hybrid",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "bing.com",
                    "cloudflare.com"
                ],
                "outboundTag": "wireguard"
            }
        ]
    },
    "transport": {
        "tlsSettings": {
            "minVersion": "1.2",
            "maxVersion": "1.3",
            "cipherSuites": "TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256:TLS_ECDHE_RSA_WITH_CHACHA20_POLY1305_SHA256"
        }
    }
}
